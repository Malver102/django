server {
    listen 80;
    server_name django-test-git-django-test.apps.lab.okd.local;  # Zmodyfikuj na swoją domenę

    location = /favicon.ico { access_log off; log_not_found off; }

    location / {
        include         uwsgi_params;
        uwsgi_pass      unix:/django/django_app.sock;
        uwsgi_param     UWSGI_CHDIR /django;
        uwsgi_param     UWSGI_SCRIPT django.wsgi:application;
    }

    location /static/ {
        alias /django/static/;
    }

    location /media/ {
        alias /django/media/;
    }

    error_page 404 /404.html;
    location = /404.html {
        root /django/;
    }